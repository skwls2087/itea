<?xml version="1.0" encoding="UTF-8"  ?>
<!-- xml선언부-->

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- element(요소)    attribute(속성성)
           최상위 element는 무조건 1개여야만 한다
 -->
 
<!-- myBatis-config.xml의     mapper로 반드시 등록되어야 한다 --> 
<mapper namespace="itLab">
	<select id="select1" parameterType="string" resultType="int">
	select result from survey where test=#{test}
	</select>
  

</mapper>














